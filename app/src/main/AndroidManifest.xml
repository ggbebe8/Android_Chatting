<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chops.android_chatting">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_chat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_chat_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Register"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".add_friend"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".tabChatMain"
            android:label="@string/title_activity_tab_chat_main" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>